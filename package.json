{
  "name": "n8n-nodes-caldav",
  "version": "0.2.2",
  "description": "Tom's attempt to build a CalDAV client node for n8n.",
  "keywords": [
    "n8n-community-node-package"
  ],
  "license": "GPL-3.0-or-later",
  "homepage": "https://github.com/that-one-tom/n8n-nodes-caldav",
  "author": {
    "name": "Thomas Martens",
    "email": "thomas@thomasmartens.eu"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/that-one-tom/n8n-nodes-caldav.git"
  },
  "main": "index.js",
  "engines": {
    "node": ">=20.15.0"
  },
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js nodes credentials package.json"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/CalDavBasicAuth.credentials.js"
    ],
    "nodes": [
      "dist/nodes/CalDav/CalDav.node.js"
    ]
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/request-promise-native": "~1.0.18",
    "@typescript-eslint/parser": "^7.3.1",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^4.0.2",
    "n8n-core": "^1.90.2",
    "n8n-workflow": "^1.90.2",
    "prettier": "^3.2.5",
    "typescript": "^5.4.2"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "node-ical": "^0.18.0",
    "ical-generator": "^6.0.1",
    "n8n-core": "^1.90.2",
    "n8n-workflow": "^1.90.2",
    "tsdav": "^2.0.7"
  },
  "bugs": {
    "url": "https://github.com/that-one-tom/n8n-nodes-caldav/issues"
  }
}
